{
    "name": "aplus/minify",
    "type": "library",
    "description": "Aplus Framework Minify package",
    "keywords": [
        "minify",
        "compression",
        "html",
        "css",
        "js",
        "javascript"
    ],
    "homepage": "https://aplus-framework.com/docs/minify",
    "license": "proprietary",
    "authors": [
        {
            "name": "Natan Felles",
            "email": "natanfelles@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-dom": "*",
        "ext-libxml": "*"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "ergebnis/composer-normalize": "^2.0",
        "natanfelles/coding-standard": "^1.0",
        "phpmd/phpmd": "^2.9",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Framework\\Minify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Minify\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-update-cmd": [
            "composer dump-autoload"
        ]
    },
    "support": {
        "email": "natanfelles@gmail.com",
        "issues": "https://gitlab.com/aplus-framework/libraries/minify/issues",
        "wiki": "https://gitlab.com/aplus-framework/libraries/minify/wikis/home",
        "source": "https://gitlab.com/aplus-framework/libraries/minify",
        "docs": "https://aplus-framework.gitlab.io/libraries/minify/docs/"
    }
}
